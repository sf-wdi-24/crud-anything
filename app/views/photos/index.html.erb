<%= render 'partials/allNav' %>
<h1>All photos</h1>
<div class="gallery" id="gallery">
  <div class="container">
    <div class="row">
    <% @photos.each do |photo| %>
        <div class="galleryimg" id="galleryimg">
          <div class="grid mix category-2 col-md-4 col-sm-6 col-xs-6">
            <figure class="effect">
              <%= image_tag photo.photo_url(:thumb).to_s %>
              <figcaption>
                <h2><%= photo.title %></h2>
                <p><%= photo.description %></p>
              </figcaption>
            </figure>
          </div>
        </div>
      <% end %>
    </div>
  </div>
</div>
