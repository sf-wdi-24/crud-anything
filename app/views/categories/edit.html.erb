<h1>edit category</h1>
<%= form_for @category do |f| %>
<%= render 'partials/flashMsg' %>

  <div class="form-group">
    <%= f.label :name %>
    <%= f.text_field :name, class:'form-control' %>
  </div>
  <div class="form-group">
    <%= f.submit 'Update Category', class:'btn btn-small btn-primary' %>
  </div>
<% end %>